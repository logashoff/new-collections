{
  "name": "new-collections",
  "description": "Browser extension to save and restore tabs",
  "version": "1.5.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --configuration production --output-hashing none",
    "postbuild": "esbuild ./src/background.ts --legal-comments=none --bundle --minify --outfile=./dist/new-collections/background.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage=true",
    "test:coverage:watch": "npm run test:coverage -- --watch",
    "ci": "npm run lint && npm run test:coverage && npm run build",
    "lint": "ng lint && stylelint ./src/**/*.scss"
  },
  "engines": {
    "node": "^18.12.1",
    "npm": "^8.19.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.8",
    "@angular/cdk": "^17.3.8",
    "@angular/common": "^17.3.8",
    "@angular/compiler": "^17.3.8",
    "@angular/core": "^17.3.8",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^17.3.8",
    "@angular/material": "^17.3.8",
    "@angular/platform-browser": "^17.3.8",
    "@angular/platform-browser-dynamic": "^17.3.8",
    "@angular/router": "^17.3.8",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "date-fns": "^3.6.0",
    "fuse.js": "^7.0.0",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1703.7",
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "^17.4.0",
    "@angular-eslint/eslint-plugin": "^17.4.0",
    "@angular-eslint/eslint-plugin-template": "^17.4.0",
    "@angular-eslint/schematics": "^17.4.0",
    "@angular-eslint/template-parser": "^17.4.0",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.8",
    "@angular/language-service": "^17.3.8",
    "@ngneat/spectator": "^18.0.1",
    "@types/chrome": "^0.0.267",
    "@types/filesystem": "^0.0.36",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "codelyzer": "^6.0.0",
    "esbuild": "^0.21.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "ng-packagr": "^17.0.0",
    "prettier": "^3.2.5",
    "protractor": "~7.0.0",
    "stylelint": "^16.3.1",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^5.4.3"
  }
}